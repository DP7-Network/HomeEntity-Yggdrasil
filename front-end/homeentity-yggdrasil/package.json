{
  "name": "homeentity-yggdrasil",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "skinview3d": "^2.0.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "yarn genFont && craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "genFont": "fsw -f SarasaGothic-UI-SC-Regular --fontPath ./src/assets/font && rm -rf fsw.html",
    "clean": "rm -rf build",
    "lint": "prettier-eslint \"*[^build]/**/*[^prod][^bundle].@(css|js|html|json)\"",
    "format": "prettier-eslint --prettier-last --write \"*[^build]/**/*[^prod][^bundle].@(css|js|html|json)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "cover 95%",
      "not dead",
      "not op_mini all",
      "not ie >= 0",
      "Firefox ESR"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9",
    "font-spider-walker": "^1.0.2",
    "postcss": "^7",
    "prettier-eslint-cli": "^5.0.1",
    "simple-progress-webpack-plugin": "^2.0.0"
  }
}
